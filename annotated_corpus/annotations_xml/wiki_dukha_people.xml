  </segment>
  <segment instanceid="wiki_dukha_people.txt_3" begin="66" end="117">
    <text>(Tozhu Tuvans) Turkic community of reindeer herders</text>
    <annotation annotator="gold" mainReferentGenericity="CANNOT_DECIDE" mainVerbAspectualClass="CANNOT_DECIDE"/>
    <annotation annotator="A" seType="" mainReferentGenericity="" habituality="" mainVerbAspectualClass=""/>
    <annotation annotator="C" seType="" mainReferentGenericity="" habituality="" mainVerbAspectualClass=""/>
    <annotation annotator="B" seType="" mainReferentGenericity="" habituality="" mainVerbAspectualClass=""/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_4" begin="118" end="164">
    <text>living in northern Khövsgöl Aimag of Mongolia.</text>
    <mainVerb begin="118" end="124">living</mainVerb>
    <mainReferent begin="110" end="117">herders</mainReferent>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="B" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_5" begin="166" end="276">
    <text>The North Taiga band was organized under the Qing Dynasty from 1755-1912 as part of Toja or Uriyankhai banner.</text>
    <mainVerb begin="191" end="200">organized</mainVerb>
    <mainReferent begin="182" end="186">band</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_6" begin="277" end="357">
    <text>With Mongolian independence in 1911, the banner became part of independent Tuva,</text>
    <mainVerb begin="325" end="331">became</mainVerb>
    <mainReferent begin="318" end="324">banner</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_7" begin="358" end="405">
    <text>which was soon annexed by the Russians in 1944,</text>
    <mainVerb begin="373" end="380">annexed</mainVerb>
    <mainReferent begin="352" end="356">Tuva</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_8" begin="406" end="474">
    <text>leaving only North Taiga band on the Mongolian side of the frontier.</text>
    <mainVerb begin="406" end="413">leaving</mainVerb>
    <mainReferent begin="318" end="324">banner</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_9" begin="475" end="601">
    <text>The South Taiga group of the Dukhans and other Uriankhais fled over the frontier from Tuva to avoid conscription in the 1930s.</text>
    <mainVerb begin="533" end="537">fled</mainVerb>
    <mainReferent begin="491" end="496">group</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_10" begin="602" end="675">
    <text>At first, the Mongolian government repeatedly deported them back to Tuva.</text>
    <mainVerb begin="648" end="656">deported</mainVerb>
    <mainReferent begin="626" end="636">government</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="GENERALIZING_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_11" begin="676" end="738">
    <text>In 1956 the government finally gave them Mongolian citizenship</text>
    <mainVerb begin="707" end="711">gave</mainVerb>
    <mainReferent begin="688" end="698">government</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_12" begin="739" end="801">
    <text>and resettled them at Tsagaan Nuur Lake on the Shishigt River.</text>
    <mainVerb begin="743" end="752">resettled</mainVerb>
    <mainReferent begin="688" end="698">government</mainReferent>
    <annotation annotator="gold" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_13" begin="803" end="833">
    <text>Only 44 Dukha families remain,</text>
    <mainVerb begin="826" end="832">remain</mainVerb>
    <mainReferent begin="817" end="825">families</mainReferent>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="B" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_14" begin="834" end="880">
    <text>totaling somewhere between 200 and 400 people.</text>
    <mainVerb begin="834" end="842">totaling</mainVerb>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="B" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_15" begin="881" end="904">
    <text>They ride, breed, milk,</text>
    <mainVerb begin="886" end="890">ride</mainVerb>
    <mainReferent begin="881" end="885">They</mainReferent>
    <annotation annotator="gold" seType="GENERALIZING_SENTENCE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="GENERALIZING_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="GENERALIZING_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_16" begin="905" end="930">
    <text>and live off of reindeer,</text>
    <mainVerb begin="909" end="913">live</mainVerb>
    <mainReferent begin="881" end="885">They</mainReferent>
    <annotation annotator="gold" seType="GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="B" seType="GENERALIZING_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_17" begin="931" end="1011">
    <text>though the reindeer population has dropped to approximately 600 since the 1970s,</text>
    <mainVerb begin="966" end="973">dropped</mainVerb>
    <mainReferent begin="951" end="961">population</mainReferent>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC:EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC:EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_18" begin="1012" end="1043">
    <text>when it was an estimated 2,000.</text>
    <mainVerb begin="1020" end="1023">was</mainVerb>
    <mainReferent begin="1017" end="1019">it</mainReferent>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_19" begin="1044" end="1181">
    <text>Since the democratization of Mongolia, no governmental programs have been in place to replenish reindeer herds with animals from Siberia,</text>
    <mainVerb begin="1113" end="1117">been</mainVerb>
    <mainReferent begin="1099" end="1107">programs</mainReferent>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="B" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_20" begin="1182" end="1223">
    <text>direly endangering the Dukha way of life.</text>
    <mainVerb begin="1189" end="1200">endangering</mainVerb>
    <annotation annotator="gold" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="STATE" mainReferentGenericity="NON-GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="B" seType="EVENT" mainReferentGenericity="NON-GENERIC" habituality="EPISODIC" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_21" begin="1224" end="1274">
    <text>Much of the Dukha income today comes from tourists</text>
    <mainVerb begin="1255" end="1260">comes</mainVerb>
    <mainReferent begin="1224" end="1228">Much</mainReferent>
    <annotation annotator="gold" seType="GENERIC_SENTENCE" mainReferentGenericity="GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="A" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
    <annotation annotator="C" seType="GENERALIZING_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="STATIC" mainVerbAspectualClass="STATIVE"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_22" begin="1275" end="1302">
    <text>who pay to buy their crafts</text>
    <mainVerb begin="1279" end="1282">pay</mainVerb>
    <mainReferent begin="1266" end="1274">tourists</mainReferent>
    <annotation annotator="gold" seType="GENERIC_SENTENCE" mainReferentGenericity="GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="A" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="C" seType="GENERALIZING_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="NON-GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
    <annotation annotator="B" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_23" begin="1303" end="1343">
    <text>and to ride their domesticated reindeer.</text>
    <mainVerb begin="1310" end="1314">ride</mainVerb>
    <annotation annotator="gold" mainReferentGenericity="CANNOT_DECIDE" mainVerbAspectualClass="CANNOT_DECIDE"/>
    <annotation annotator="A" seType="" mainReferentGenericity="" habituality="" mainVerbAspectualClass=""/>
