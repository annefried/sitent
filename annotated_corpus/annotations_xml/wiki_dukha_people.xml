    <annotation annotator="B" seType="GENERIC_SENTENCE:GENERAL_STATIVE" mainReferentGenericity="GENERIC" habituality="HABITUAL" mainVerbAspectualClass="DYNAMIC"/>
  </segment>
  <segment instanceid="wiki_dukha_people.txt_24" begin="1344" end="1415">
    <text>The name "tsaatan" means "reindeer herder" as in "tsaa bug" (reindeer).</text>
